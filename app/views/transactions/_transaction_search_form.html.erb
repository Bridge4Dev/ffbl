<h4 class="search-title">Search Individual Team or Player Transactions</h4>
<%= form_tag transactions_path, :method => "get" do %>
	<%= text_field_tag :search, params[:search], placeholder: "Team or Player", class: "search-text" %>
	<%= submit_tag "Search", :name => nil, class: "btn btn-cancel" %>
<% end %>