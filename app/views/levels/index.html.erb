<%= provide(:title, "All Levels") %>
<div class="container content-container">
	<div class="row center">
		<div class="col-sm-12">
			<h1>All Levels</h1>
		</div>
		<div class="col-sm-4 col-sm-offset-4">
			<table class="table">
				<% @levels.each do |level| %>
				<tr>
					<td><h3><%= level.id %>:  <%= level.league %></h3></td>
				</tr>
				<% end %>
			</table>
		</div>
	</div>
</div>